    agent any 
    stages {
        stage('Build') { 
            steps {
                echo "I am in 1111 stage" 
            }
        }
        stage('Test') { 
            steps {
                echo "I am in 2222 stage" 
            }
        }
        stage('Deploy') { 
            steps {
                echo "I am 3333 stage"
            }
        }
        stage('Archive') { 
            steps {
                echo "I am 4444 stage"
            }
        }
    }
}
